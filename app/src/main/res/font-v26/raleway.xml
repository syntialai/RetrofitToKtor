<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <font
        android:font="@font/raleway_extra_light"
        android:fontStyle="normal"
        android:fontWeight="200"
        app:fontStyle="normal"
        app:fontWeight="200"
        app:font="@font/raleway_extra_light" />

    <font
        android:font="@font/raleway_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/raleway_regular" />

    <font
        android:font="@font/raleway_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:fontStyle="normal"
        app:fontWeight="500"
        app:font="@font/raleway_medium" />

    <font
        android:font="@font/raleway_semi_bold"
        android:fontStyle="normal"
        android:fontWeight="600"
        app:fontStyle="normal"
        app:fontWeight="600"
        app:font="@font/raleway_semi_bold" />

    <font
        android:font="@font/raleway_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/raleway_bold" />
</font-family>